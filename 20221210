def solution(n):
    remain = []
    answer = 0
    while n != 0:
        remain.append(n % 3)
        n = n//3
        #remain = ['0','0','2','1']

        #string = '0021'
    l = 0
    remain.reverse()
    for j in range(len(remain)):
        if l == 0:
            answer += remain[j]
            l += 1
        else:
            answer += remain[j] * (3 ** l)
            l+=1
    return answer



